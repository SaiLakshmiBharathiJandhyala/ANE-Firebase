<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.distriqt.Firebase</id>
	<versionNumber>1.0.066</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.firebase.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.firebase.FirebaseExtension</initializer>
				<finalizer>com.distriqt.extension.firebase.FirebaseExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.firebase.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.firebase.FirebaseExtension</initializer>
				<finalizer>com.distriqt.extension.firebase.FirebaseExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libFirebase.a</nativeLibrary>
				<initializer>FirebaseExtInitializer</initializer>
				<finalizer>FirebaseExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libFirebase.a</nativeLibrary>
				<initializer>FirebaseExtInitializer</initializer>
				<finalizer>FirebaseExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default"> 
			<applicationDeployment /> 
		</platform>
	</platforms>
</extension>
